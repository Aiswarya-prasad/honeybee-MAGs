rule checkm_evaluation:
    input: 06_MAG_binning/bins_renamed/D3.5.done
    output: 06_MAG_binning/evaluate_bins/D3.5_checkm.summary, 06_MAG_binning/evaluate_bins/D3.5/lineage.ms
