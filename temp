rule map_to_MAGs:
    input: 01_Trimmed/DrY2_N1_R1_trim.fastq.gz, 01_Trimmed/DrY2_N1_R2_trim.fastq.gz, database/MAGs_database, database/MAGs_database.amb, database/MAGs_database.ann, database/MAGs_database.bwt, database/MAGs_database.pac, database/MAGs_database.sa
    output: 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1.bam, 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1.sam, 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1_flagstat.tsv
    log: logs/DrY2_N1_map_to_mags.log
    jobid: 1427
    benchmark: logs/DrY2_N1_map_to_mags.benchmark
    reason: Missing output files: 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1_flagstat.tsv; Input files updated by another job: 01_Trimmed/DrY2_N1_R1_trim.fastq.gz, database/MAGs_database.ann, database/MAGs_database.amb, database/MAGs_database.bwt, 01_Trimmed/DrY2_N1_R2_trim.fastq.gz, database/MAGs_database, database/MAGs_database.sa, database/MAGs_database.pac
    wildcards: sample=DrY2_N1
    threads: 8
    resources: mem_mb=20000, disk_mb=<TBD>, tmpdir=/tmp


        bwa mem -t 8 database/MAGs_database 01_Trimmed/DrY2_N1_R1_trim.fastq.gz 01_Trimmed/DrY2_N1_R2_trim.fastq.gz > 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1.sam
        samtools view -bh 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1.sam | samtools sort - > 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1.bam
        samtools flagstat -O tsv 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1.bam > 09_MagDatabaseProfiling/MAGsDatabaseMapping/DrY2_N1_flagstat.tsv
        

