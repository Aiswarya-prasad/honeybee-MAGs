    output: 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4.bam, 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4.sam, 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4_flagstat.tsv
    log: logs/F1.4_map_to_rep_MAGs.log
    jobid: 529
    benchmark: logs/F1.4_map_to_rep_MAGs.benchmark
    reason: Missing output files: 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4.bam; Input files updated by another job: 10_instrain/rep_mags.fasta
    wildcards: sample=F1.4
    threads: 8
    resources: mem_mb=20000, disk_mb=<TBD>, tmpdir=/tmp


        bwa index 10_instrain/rep_mags.fasta
        bwa mem -t 8 10_instrain/rep_mags.fasta 01_Trimmed/F1.4_R1_trim.fastq.gz 01_Trimmed/F1.4_R2_trim.fastq.gz > 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4.sam
        samtools view -bh 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4.sam | samtools sort - > 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4.bam
        samtools index 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4.bam
        samtools flagstat -O tsv 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4.bam > 09_MagDatabaseProfiling/SNVProfiling/Mapping/F1.4_flagstat.tsv
        

