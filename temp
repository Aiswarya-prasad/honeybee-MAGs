[Wed Nov  2 10:57:22 2022]
rule map_to_rep_MAGs:
    input: 01_Trimmed/D1.5_R1_trim.fastq.gz, 01_Trimmed/D1.5_R2_trim.fastq.gz, 10_instrain/rep_mags.fasta
    output: 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5.bam, 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5.sam, 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5_flagstat.tsv
    log: logs/D1.5_map_to_rep_MAGs.log
    jobid: 501
    benchmark: logs/D1.5_map_to_rep_MAGs.benchmark
    reason: Missing output files: 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5.bam; Input files updated by another job: 10_instrain/rep_mags.fasta
    wildcards: sample=D1.5
    threads: 8
    resources: mem_mb=20000, disk_mb=<TBD>, tmpdir=/tmp


        bwa index 10_instrain/rep_mags.fasta
        bwa mem -t 8 10_instrain/rep_mags.fasta 01_Trimmed/D1.5_R1_trim.fastq.gz 01_Trimmed/D1.5_R2_trim.fastq.gz > 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5.sam
        samtools view -bh 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5.sam | samtools sort - > 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5.bam
        samtools index 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5.bam
        samtools flagstat -O tsv 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5.bam > 09_MagDatabaseProfiling/SNVProfiling/Mapping/D1.5_flagstat.tsv
        

