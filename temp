                mag_name=$(basename ${mag})
                echo "running parse_stb for ${mag}"
                parse_stb.py --reverse -f ${mag} -o 10_instrain/${mag_name%%.fna}_stb.temp
            done
            cat 10_instrain/*_stb.temp > 10_instrain/rep_mags_stb.tsv
            rm 10_instrain/*_stb.temp
        fi
        if [ ! -f 10_instrain/rep_mags.fasta ];
        then
            awk ' /^>/ { printf("\n%s\n",$0);next; } { printf("%s",$0); } END { printf("\n"); }' database/MAGs_rep_database | tail -n +2 > 10_instrain/rep_mags.fasta
        fi
        if [ ! -f 10_instrain/rep_mags_genes.fna ];
        then
            prodigal -p meta -i 10_instrain/rep_mags.fasta -d 10_instrain/rep_mags_genes.fna -o logs/prep_for_instrain.log
        fi
        

